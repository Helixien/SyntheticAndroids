	<!--=============== New Implants (Androids Only)	====================-->

	<!-- Advanced Fluid Processor -->
	<HediffDef ParentName="AddedBodyPartBase">
		<defName>SA_AdvancedFluidProcessor</defName>
		<label>advanced fluid processor</label>
		<labelNoun>an advanced fluid processor</labelNoun>
		<description>An installed advanced fluid processor.</description>
		<descriptionHyperlinks>
			<ThingDef>SA_AdvancedFluidProcessor</ThingDef>
		</descriptionHyperlinks>
		<spawnThingOnRemoved>SA_AdvancedFluidProcessor</spawnThingOnRemoved>
		<addedPartProps>
			<solid>true</solid>
			<partEfficiency>1.25</partEfficiency>
			<betterThanNatural>true</betterThanNatural>
		</addedPartProps>
	</HediffDef>

	<ThingDef ParentName="BodyPartBionicBase">
		<defName>SA_AdvancedFluidProcessor</defName>
		<label>advanced fluid processor</label>
		<description>ph</description>
		<descriptionHyperlinks>
			<RecipeDef>SA_InstallAdvancedFluidProcessor</RecipeDef>
		</descriptionHyperlinks>
		<costList>
			<Plasteel>15</Plasteel>
			<ComponentSpacer>4</ComponentSpacer>
		</costList>
	</ThingDef>

	<RecipeDef ParentName="SA_SurgeryInstallBodyPartArtificialBase">
